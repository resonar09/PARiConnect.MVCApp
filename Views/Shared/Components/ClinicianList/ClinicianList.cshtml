     
@model PARiConnect.MVCApp.ViewModels.ClinicianViewModel     
      <div class="pt-3">
        <div class="card small ">
          <div class="card-header font-weight-bold">
            Clinician <a href="#" class="float-right "><small>select all</small></a>
          </div>
            <div class="card-block p-2">
              <div class="card-body">
                      <ul class="list-group list-group-flush">
                      @foreach (var clinician in Model.Clinicians)
                        {      
                        <li class="list-group-item">
                            <div class="form-check">
                              <input type="checkbox" class="form-check-input" id="exampleCheck1">
                              <label class="form-check-label" for="exampleCheck1">@clinician.Name</label>
                            </div>
                        </li>
                        }
                      </ul>
              </div>

            </div>
        </div>
      </div>